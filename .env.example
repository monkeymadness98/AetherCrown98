# PayPal Configuration
# Get your credentials from https://developer.paypal.com/
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here

# Supabase Configuration
# Create a project at https://supabase.com/ and get your credentials
SUPABASE_URL=https://your_project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Render Configuration
# Get your API key from https://render.com/ dashboard
RENDER_API_KEY=your_render_api_key_here

# Vercel Configuration (for deployment)
# Get these from https://vercel.com/ dashboard
VERCEL_TOKEN=your_vercel_token_here
VERCEL_TEAM_ID=your_vercel_team_id_here

# General Environment
NODE_ENV=development
PORT=3000
